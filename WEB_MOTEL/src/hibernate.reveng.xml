<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
    <type-mapping>
        <sql-type jdbc-type="NUMERIC" precision='1' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='2' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='3' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='4' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='5' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='6' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='7' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='8' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='9' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='10' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" precision='11' hibernate-type="Integer"/>
        <sql-type jdbc-type="NUMERIC" hibernate-type="Long"/>

        <sql-type jdbc-type="VARCHAR" hibernate-type="string"/>
        <sql-type jdbc-type="CHAR" hibernate-type="string"/>

        <sql-type jdbc-type="TIMESTAMP" hibernate-type="java.sql.Timestamp"/>
        <sql-type jdbc-type="DATE" hibernate-type="java.sql.Timestamp"/>
        <sql-type jdbc-type="OTHER" hibernate-type="java.sql.Timestamp"/>
    </type-mapping>
</hibernate-reverse-engineering>